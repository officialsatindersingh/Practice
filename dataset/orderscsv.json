{
	"name": "orderscsv",
	"properties": {
		"linkedServiceName": {
			"referenceName": "AzureBlobStorage",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"container": {
				"type": "string"
			},
			"folder": {
				"type": "string"
			},
			"file": {
				"type": "string"
			}
		},
		"annotations": [],
		"type": "DelimitedText",
		"typeProperties": {
			"location": {
				"type": "AzureBlobStorageLocation",
				"fileName": {
					"value": "@dataset().file",
					"type": "Expression"
				},
				"folderPath": {
					"value": "@dataset().folder",
					"type": "Expression"
				},
				"container": {
					"value": "@dataset().container",
					"type": "Expression"
				}
			},
			"columnDelimiter": ",",
			"escapeChar": "\\",
			"firstRowAsHeader": true,
			"quoteChar": "\""
		},
		"schema": [
			{
				"name": "orderid",
				"type": "String"
			},
			{
				"name": "orderamount",
				"type": "String"
			}
		]
	}
}